# GitHub Personal Access Token (required)
# Replace 'your-github-token-here' with a valid token with repo access
GITHUB_TOKEN=your-github-token-here

# GitHub Repository in 'owner/repo' format
GITHUB_REPO=cosminmemetea/dashboards

# Milestone title for burndown chart
MILESTONE_TITLE=Milestone I

# Project title for burndown chart
PROJECT_TITLE=Dashboard Project

# Default sprint name
SPRINT_NAME=Sprint 1

# Name of the sprint field in GitHub Projects (optional, defaults to "Sprint")
SPRINT_FIELD_NAME=Sprint